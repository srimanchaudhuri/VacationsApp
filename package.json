  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.4",
  "dependencies": {
    "bcryptjs": "^3.0.0",
    "esmodule": "^0.2.1",
    "jsonwebtoken": "^9.0.2"
  }
}
